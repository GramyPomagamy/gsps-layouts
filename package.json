    "@types/request-promise": "^4.1.48",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cheerio": "1.0.0-rc.12",
    "del": "^7.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "get-port": "^7.0.0",
    "globby": "^13.1.4",
    "nodecg-cli": "^8.6.6",
    "npm-run-all": "^4.1.5",
