    "webpack-cli": "^4.7.2",
    "webpack-livereload-plugin": "^3.0.1"
  },
  "nodecg": {
    "compatibleRange": "^1.8.1",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.4.0"
    },
    "dashboardPanels": [
      {
        "name": "total",
        "title": "Zebrana kwota",
        "file": "total.html",
        "width": 3
      },
      {
        "name": "countdown",
        "title": "Odliczanie",
        "file": "countdown.html",
        "width": 3
      },
      {
        "name": "edit-total",
        "title": "Edytuj kwotÄ™",
        "file": "edit-total.html",
        "width": 3,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "zapisz",
            "type": "confirm"
          },
          {
            "name": "anuluj",
            "type": "dismiss"
          }
        ]
      }
    ],
    "graphics": [
      {
        "file": "countdown.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "omnibar.html",
        "width": 1920,
        "height": 66
      },
      {
