      },
      {
        "name": "media-box-controls",
        "title": "3. GSPS - Kontrola mediaboxa",
        "file": "media-box-controls.html",
        "fullbleed": true
      }
    ],
    "graphics": [
      {
        "file": "odliczanie.html",
        "width": 1920,
        "height": 1030
      },
