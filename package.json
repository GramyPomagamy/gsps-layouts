{
  "name": "gsps-layouts",
  "version": "2.0.0",
  "description": "Layouts used for the GSPS (Gramy Szybko, Pomagamy Skutecznie) events, based on a template for NodeCG bundles that use Vue.js and TypeScript by zoton2.",
  "homepage": "https://github.com/GramyPomagamy/gsps-layouts",
  "bugs": {
    "url": "https://github.com/GramyPomagamy/gsps-layouts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GramyPomagamy/gsps-layouts.git"
  },
  "license": "MIT",
  "author": "GramyPomagamy",
  "contributors": [
    "hoxi",
    "florczakraf"
  ],
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.tsx src/browser",
    "autofix:extension": "eslint --fix --ext .ts src/extension && eslint --fix --ext .d.ts src/nodecg",
    "build": "npm run clean && vite build",
    "clean": "trash node_modules/.cache && trash shared/dist && trash dashboard && trash graphics && trash extension",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettify": "prettier --write src/browser src/extension",
    "prebuild": "npm run clean",
    "schema-types": "nodecg schema-types -o src/types/generated",
    "start": "node ../..",
    "watch": "run-p watch:*",
    "watch:vite": "vite",
    "watch:tsc": "tsc -b tsconfig.extension.json -w --preserveWatchOutput"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "@vitejs/plugin-react": "^4.0.0",
    "auto-text-size": "github:hoXyy/auto-text-size",
    "bluebird": "^3.7.2",
    "csv-stringify": "^6.4.0",
    "dayjs": "^1.11.8",
    "deep-equal": "^2.2.1",
    "events": "^3.3.0",
    "google-spreadsheet": "^3.3.0",
    "gsap": "^3.12.1",
    "humanize-duration": "^3.28.0",
    "klona": "^2.0.6",
    "livesplit-core": "^0.10.2",
    "milliseconds": "^1.0.3",
    "moment": "^2.29.4",
    "needle": "^3.2.0",
    "numeral": "^2.0.6",
    "obs-websocket-js": "^5.0.3",
    "parse-ms": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-transition-group": "^4.4.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "socket.io-client": "^4.6.2",
    "styled-components": "6.0.0-rc.3",
    "svg.js": "^2.7.1",
    "use-nodecg": "0.4.1-2"
  },
  "devDependencies": {
    "@nodecg/types": "^2.1.8",
    "@types/bluebird": "^3.5.38",
    "@types/deep-equal": "^1.0.1",
    "@types/google-spreadsheet": "^3.3.2",
    "@types/humanize-duration": "^3.27.1",
    "@types/milliseconds": "^0.0.30",
    "@types/needle": "^3.2.0",
    "@types/numeral": "^2.0.2",
