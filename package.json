{
  "name": "gsps-layouts",
  "version": "1.0.0",
  "description": "Layouts used for the GSPS (Gramy Szybko, Pomagamy Skutecznie) events, based on a template for NodeCG bundles that use Vue.js and TypeScript by zoton2.",
  "homepage": "https://github.com/zoton2/nodecg-vue-ts-template#readme",
  "bugs": {
    "url": "https://github.com/zoton2/nodecg-vue-ts-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoton2/nodecg-vue-ts-template.git"
  },
  "license": "MIT",
  "author": "GramyPomagamy",
  "contributors": [
    "hoxi"
  ],
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.vue src/dashboard && eslint --fix --ext .ts,.vue src/graphics && eslint --fix --ext .ts src/browser_shared",
    "autofix:extension": "eslint --fix --ext .ts src/extension && eslint --fix --ext .d.ts src/types",
    "build": "run-s build:*",
    "build:browser": "cross-env NODE_ENV=production webpack",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash extension",
    "schema-types": "nodecg schema-types",
    "start": "node ../..",
    "watch": "run-p watch:*",
    "watch:browser": "webpack -w",
    "watch:extension": "tsc -b tsconfig.extension.json -w"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/deep-equal": "^1.0.1",
    "@types/events": "^3.0.0",
    "@types/milliseconds": "^0.0.30",
    "@types/numeral": "^2.0.1",
    "@types/request": "^2.48.6",
    "@types/request-promise": "^4.1.48",
    "bluebird": "^3.7.2",
    "deep-equal": "^2.0.5",
    "events": "^3.3.0",
    "gsap": "^3.7.1",
    "milliseconds": "^1.0.3",
    "module-alias": "^2.2.2",
    "numeral": "^2.0.6",
    "parse-ms": "^2.1.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "socket.io-client": "^2.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@types/clone": "^2.1.0",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^16.4.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vue/component-compiler-utils": "^3.2.2",
    "clone": "^2.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "deepmerge": "^4.2.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.11.1",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "globby": "^11.0.4",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "nodecg-cli": "git+https://git@github.com/zoton2/nodecg-cli.git#d7c7c5673753441d254c31d4f980727cd42505f9",
    "npm-run-all": "^4.1.5",
    "sass": "~1.32",
    "sass-loader": "^12.1.0",
    "trash-cli": "^4.0.0",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.4",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.9.7",
    "vue-property-decorator": "^9.1.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
