{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "name": { "type": "string" },
      "pronouns": {
        "type": "string",
        "enum": [
          "ona/jej",
          "on/jego",
          "ono/jego",
          "ono/jej",
          "ono/jejgo",
          "ono/jeno",
          "ono/jenu",
          "ono/ich",
          "onu/jenu",
          "onu/jejo",
          "ne/nego",
          "onu/jegu",
          ""
        ]
      },
      "channel": {
        "type": "string",
        "enum": [
          "",
          "H1",
          "H2",
          "H3",
          "H4",
          "Host1",
          "Host2",
          "Host3",
          "Donacje",
          "Gra1",
          "Gra2"
        ],
        "default": ""
      }
    },
    "required": ["name", "pronouns", "channel"]
  },
  "default": []
}
