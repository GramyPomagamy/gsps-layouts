{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "name": { "type": "string" },
      "pronouns": {
        "type": "string",
        "enum": [
          "she/her",
          "he/him",
          "they/she",
          "they/them",
          ""
        ]
      }
    },
    "required": ["name", "pronouns"]
  },
  "default": []
}
