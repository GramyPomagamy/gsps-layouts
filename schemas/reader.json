{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": { "type": "string", "default": "" },
    "pronouns": {
      "type": "string",
      "enum": [
        "she/her",
        "he/him",
        "they/she",
        "they/them",
        ""
      ],
      "default": ""
    }
  },
  "required": ["name", "pronouns"]
}
