{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "number"
      },
      "name": {
        "type": "string"
      },
      "amount": {
        "type": "number"
      },
      "comment": {
        "type": "string"
      },
      "timestamp": {
        "type": "number"
      },
      "bid": {
        "type": "array",
        "items": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "id": {
              "type": "number"
            },
            "amount": {
              "type": "number"
            }
          }
        }
      }
    },
    "required": ["id", "name", "amount", "timestamp"]
  },
  "default": []
}
