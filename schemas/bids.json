{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "number"
      },
      "name": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "longDescription": {
        "type": "string"
      },
      "total": {
        "type": "string"
      },
      "rawTotal": {
        "type": "number"
      },
      "game": {
        "type": "string"
      },
      "category": {
        "type": "string"
      },
      "runStartTime": {
        "type": "number"
      },
      "runEndTime": {
        "type": "number"
      },
      "type": {
        "type": "string",
        "enum": ["challenge", "choice"]
      },
      "public": {
        "type": "string"
      },
      "allowUserOptions": {
        "type": "boolean"
      },
      "options": {
        "type": "array",
        "items": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "id": {
              "type": "number"
            },
            "parent": {
              "type": "number"
            },
            "name": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "total": {
              "type": "string"
            },
            "rawTotal": {
              "type": "number"
            },
            "speedrun": {
              "type": "number"
            }
          },
          "required": ["id", "parent", "name", "description", "total", "rawTotal", "speedrun"]
        }
      },
      "goal": {
        "type": "number"
      },
      "rawGoal": {
        "type": "number"
      }
    },
    "required": ["id", "name", "total", "rawTotal", "type", "allowUserOptions", "options", "public"]
  },
  "default": []
}
